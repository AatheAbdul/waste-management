<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg xmlns="http://www.w3.org/2000/svg" width="1920" height="1080" viewBox="0 0 1920 1080">
  <defs>
    <linearGradient id="overlay" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#1B5E20;stop-opacity:0.8"/>
      <stop offset="100%" style="stop-color:#1B5E20;stop-opacity:0.8"/>
    </linearGradient>
    <pattern id="pattern-circles" x="0" y="0" width="40" height="40" patternUnits="userSpaceOnUse">
      <circle cx="20" cy="20" r="15" fill="#4CAF50" fill-opacity="0.1"/>
    </pattern>
  </defs>
  
  <!-- Background with gradient -->
  <rect width="100%" height="100%" fill="#2E7D32"/>
  
  <!-- Decorative pattern overlay -->
  <rect width="100%" height="100%" fill="url(#pattern-circles)"/>
  
  <!-- Gradient overlay -->
  <rect width="100%" height="100%" fill="url(#overlay)"/>
  
  <!-- Abstract waste management symbols -->
  <g transform="translate(960, 540)" fill="#FFFFFF" fill-opacity="0.1">
    <!-- Recycling arrows -->
    <path d="M-200,-100 L-150,-50 L-250,-50 Z" transform="rotate(0)"/>
    <path d="M-200,-100 L-150,-50 L-250,-50 Z" transform="rotate(120)"/>
    <path d="M-200,-100 L-150,-50 L-250,-50 Z" transform="rotate(240)"/>
    
    <!-- Leaf symbol -->
    <path d="M100,0 Q150,-50 200,0 T300,0 Q200,100 100,0 Z"/>
    
    <!-- Compost bin symbol -->
    <rect x="-350" y="100" width="100" height="150" rx="10"/>
    <path d="M-330,100 L-270,50 L-210,100" fill="none" stroke="#FFFFFF" stroke-opacity="0.1" stroke-width="5"/>
  </g>
</svg>